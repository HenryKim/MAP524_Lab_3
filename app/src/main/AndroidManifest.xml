<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="hkim142.map524lab3" >
    <uses-permission android:name="android.permission.INTERNET" />
    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".ActionAbout"
            android:label="About:" >
            <intent-filter>
                <action android:name="android.intent.action.ActionAbout" />
            </intent-filter>
        </activity>
        <activity
            android:name=".ActionHelp"
            android:label="HELP!" >
            <intent-filter>
                <action android:name="android.intent.action.ActionHelp" />
            </intent-filter>
        </activity>
        <activity
            android:name=".ActivityTerminology"
            android:label="Terminology">
            <intent-filter>
                <action android:name="android.intent.action.ActivityTerminology" />
            </intent-filter>
        </activity>
        <activity
            android:name=".ActivityWebsites"
            android:label="WebSite">
            <intent-filter>
                <action android:name="android.intent.action.ActivityWebsites" />
            </intent-filter>
        </activity>


    </application>

</manifest>
